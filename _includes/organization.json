{
  "@context": "https://schema.org",
  "@type": "Organization",
  "@id": "{{ "/#organization" |  preprend site.baseurl | prepend: site.url }}",
  "url": "{{ site.url }}",
  "name": "{{ site.company_name }}",
  "logo": "{{ "/images/logo-a10s.png" | preprend site.baseurl | prepend: site.url }}",
  "sameAs": ["https://twitter.com/a10s_ca"],
  "contactPoint": [
    { "@type": "ContactPoint",
      "email": "{{ site.email }}",
      "contactType": "customer service",
      "areaServed": [
        "US",
        "CA",
        "FR"
      ],
      "availableLanguage": [
        "English",
        "French"
      ]
    },
    { "@type": "ContactPoint",
      "email": "{{ site.email }}",
      "contactType": "sales",
      "areaServed": [
        "US",
        "CA",
        "FR"
      ],
      "availableLanguage": [
        "English",
        "French"
      ]
    }
  ],
  "founder": [
    {
      "@type": "Person",
      "@id": "{{ "/equipe#christian_roy" |  preprend site.baseurl | prepend: site.url }}",
      "name": "Christian Roy",
      "url": "http://unchristianroy.com/",
      "sameAs": [
        "https://twitter.com/christianroy",
        "https://www.linkedin.com/in/christianroyquebec/",
        "http://unchristianroy.com/",
        "https://medium.com/@christian_roy",
        "https://github.com/christianroy",
        "https://christianroy.tumblr.com/"
      ]
    },
    {
      "@type": "Person",
      "@id": "{{ "/equipe#clement_laberge" |  preprend site.baseurl | prepend: site.url }}",
      "name": "Cl√©ment Laberge",
      "url": "https://remolino.qc.ca/",
      "sameAs": [
        "https://remolino.qc.ca/",
        "https://twitter.com/remolino",
        "https://www.linkedin.com/in/claberge/"
      ]
    }
  ]
}
